@using MVC4.Bootstrapped.Helpers
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    @Html.RenderStyles()
    @Html.RenderMobileStyles()
    @Html.RenderScripts()
    @Html.RenderMobileScripts()
    @RenderSection("head-scripts", required: false)
</head>
<body>
    @RenderBody()

    @RenderSection("body-scripts", required: false)
</body>
</html>
